{
  "name": "page-meta-module",
  "version": "1.0.0",
  "private": true,
  "description": "This module provides the `definePageMeta` macro in Nuxt 2 as well.",
  "repository": "https://github.com/wattanx/page-meta-module",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "FORCE_COLOR=1 pnpm --filter './packages/**' prepack",
