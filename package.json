    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@nuxt/kit": "3.7.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
