    "@nuxt/schema": "3.7.0",
    "@nuxt/test-utils": "3.7.0",
    "@types/node": "^18",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitest/ui": "0.34.3",
    "@vue/test-utils": "^1.3.6",
    "changelogen": "^0.5.3",
    "eslint": "8.48.0",
    "jsdom": "^22.1.0",
    "vitest": "0.34.3",
    "vue": "2.7.14"
  }
}
